RESTART
UNITS ENERGY=kcal/mol
phi: TORSION ATOMS=5,7,9,15
psi: TORSION ATOMS=7,9,15,17

metad: METAD ARG=phi,psi PACE=10000 HEIGHT=0.3 SIGMA=0.25,0.25 BIASFACTOR=5 TEMP=300.0 FILE=HILLS ACCELERATION ACCELERATION_RFILE=COLVAR GRID_MIN=-pi,-pi GRID_MAX=pi,pi GRID_BIN=180,180 GRID_WSTRIDE=5000 GRID_WFILE=BIAS.dat 

COMMITTOR ...
  ARG=phi
  STRIDE=100
  BASIN_LL1=0.5
  BASIN_UL1=1.5

  FILE=commit.log FMT=%8.4f
... COMMITTOR
 
PRINT STRIDE=100  ARG=phi,psi,metad.bias,metad.acc FILE=COLVAR1


